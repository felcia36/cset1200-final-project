           temp.setCompletedCourseHours(sc.nextInt());
           allCourses.set(i, temp);
       }
      
       for(int i = 0;i < allCourses.size();i++){
           temp = allCourses.get(i);
           System.out.println("You have " + (temp.getTotalCourseHours() - temp.getCompletedCourseHours()) + " " + temp.getCourseName() + " credit hours remaining.");  
           totalCreditHoursCompleted += temp.getCompletedCourseHours();
       }
      
       System.out.println("You have " + (128 - totalCreditHoursCompleted) + " CSET credit hours remaining.");
      
       if(totalCreditHoursCompleted == 128){
           System.out.println("Congratulations!!! You have comleted all your Course work.");
